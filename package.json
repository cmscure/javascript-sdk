{
  "name": "@cmscure/javascript-sdk",
  "version": "1.2.4",
  "description": "Official JavaScript SDK for CMSCure content management platform",
  "type": "module",
  "main": "dist/cmscure.js",
  "module": "dist/cmscure.esm.js",
  "browser": "dist/cmscure.umd.js",
  "unpkg": "dist/cmscure.umd.min.js",
  "jsdelivr": "dist/cmscure.umd.min.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "prepublishOnly": "npm run build:prod",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "cms",
    "content-management",
    "localization",
    "i18n",
    "translations",
    "javascript",
    "sdk",
    "react",
    "nextjs",
    "web"
  ],
  "author": {
    "name": "CMSCure",
    "email": "support@cmscure.com",
    "url": "https://cmscure.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmscure/javascript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/cmscure/javascript-sdk/issues"
  },
  "homepage": "https://github.com/cmscure/javascript-sdk#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.12.0"
  },
  "dependencies": {},
  "peerDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
